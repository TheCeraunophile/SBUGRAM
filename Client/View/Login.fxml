<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Controller.LoginController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="600.0" prefWidth="900.0">
         <children>
            <CheckBox fx:id="showPasswordButton" layoutX="513.0" layoutY="198.0" mnemonicParsing="false" onAction="#showPassword" text="show" />
            <Button fx:id="login_button" layoutX="372.0" layoutY="460.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="95.0" text="LogIn" />
            <Button fx:id="Signup_button" layoutX="372.0" layoutY="492.0" mnemonicParsing="false" onAction="#signup" prefHeight="25.0" prefWidth="95.0" text="Signup" />
            <Button fx:id="needhelp_button" layoutX="372.0" layoutY="525.0" mnemonicParsing="false" onAction="#needhellp" prefHeight="25.0" prefWidth="95.0" text="need help" />
            <TextField fx:id="username_field" layoutX="334.0" layoutY="158.0" promptText="username" />
            <TextField fx:id="year_test" layoutX="334.0" layoutY="356.0" prefHeight="25.0" prefWidth="65.0" promptText="YY" />
            <TextField fx:id="month_test" layoutX="404.0" layoutY="356.0" prefHeight="25.0" prefWidth="46.0" promptText="MM" />
            <TextField fx:id="day_test" layoutX="458.0" layoutY="356.0" prefHeight="25.0" prefWidth="46.0" promptText="DD" />
            <CheckBox layoutX="702.0" layoutY="232.0" mnemonicParsing="false" text="show" visible="false" />
            <TextField fx:id="name_hellp" layoutX="14.0" layoutY="158.0" prefHeight="25.0" prefWidth="230.0" promptText="write your name" visible="false" />
            <TextField fx:id="broken_pass" layoutX="14.0" layoutY="194.0" prefHeight="25.0" prefWidth="230.0" promptText="write some of your password" visible="false" />
            <TextField fx:id="day_birthday" layoutX="14.0" layoutY="228.0" prefHeight="25.0" prefWidth="230.0" promptText="write number of day birthday" visible="false" />
            <PasswordField fx:id="password_field" layoutX="334.0" layoutY="194.0" promptText="password" />
            <TextArea fx:id="bio_field" layoutX="334.0" layoutY="232.0" prefHeight="107.0" prefWidth="170.0" promptText="bio" />
            <TextField fx:id="show_password" editable="false" layoutX="334.0" layoutY="194.0" visible="false" />
            <Label fx:id="logOfFailed" alignment="CENTER" contentDisplay="CENTER" layoutX="308.0" layoutY="5.0" prefHeight="18.0" prefWidth="238.0" text="password or username NOT correct" textAlignment="CENTER" textFill="RED" textOverrun="CENTER_ELLIPSIS" visible="false" />
            <Label fx:id="logOfUsernameFailed" alignment="CENTER" contentDisplay="CENTER" layoutX="225.0" layoutY="7.0" prefHeight="15.0" prefWidth="467.0" text="username should only contains characters 0-9 numbers and underLine" textAlignment="CENTER" textFill="RED" visible="false" />
         </children>
      </Pane>
   </children>
</GridPane>
